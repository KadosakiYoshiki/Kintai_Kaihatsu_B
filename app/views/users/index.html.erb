<% provide(:title, '一覧') %>
<h1>ユーザー一覧</h1>
<div class="page-right">
  <p>ユーザー名検索</p>
    <%= form_tag(users_path, method: "get") do %>
      <%= text_field_tag :search %>
      <%= submit_tag 'Search', :name => nil %>
    <% end %>
</div>
<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>